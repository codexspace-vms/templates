{
  "name": "Apache Cordova",
  "description": "Build Android apps with HTML, CSS, and JavaScript using Apache Cordova",
  "categories": [
    "Mobile"
  ],
  "icon":  "https://cordova.apache.org/static/img/cordova_256. png",
  "publisher": "Shiman",
  "host": {
    "virtualization": true
  },
  "params": [
    {
      "id":  "appName",
      "name": "What is your app name?",
      "type": "text",
      "default": "MyApp",
      "required": true
    },
    {
      "id": "appId",
      "name": "What is your app ID (reverse domain)?",
      "type": "text",
      "default":  "com.example.myapp",
      "required": true
    }
  ]
}